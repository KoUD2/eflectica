    <div class="W_Effects">
      <div class="C_Effects">
        <% @effects.first(3).each do |effect| %>
          <%= render partial: "effect", locals: { effect: effect } %>
        <% end %>
      </div>
      <div class="C_Effects">
        <% @effects.offset(3).first(3).each do |effect| %>
